  "can_delegate_to": [
    "sui-core-agent",
    "sui-infrastructure-agent",
    "sui-development-agent",
    "sui-ecosystem-agent"
  ],
  "tools_available": [
    "Task",
    "Skill",
    "Read",
    "Write",
    "Glob",
    "Grep",
    "Bash"
  ],
  "state_schema": "./state-schema.json",
  "prompt_template": "./sui-supreme-prompt.md"
}
